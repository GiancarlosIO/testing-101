function mySuperImportantFuncion(num1, numb2, operator) {
  if (operator === 'm') return num1 * numb2;
  if (operator === 'r') return num1 - numb2;
  return numb2 + numb2;
};

// tests

it('should work as expected', () => {
  const result = mySuperImportantFuncion(1, 2);

  // asserts
  expect(result).toBe(3);
});

// 100% coverage

it('should return 2', () => {
  const result = mySuperImportantFuncion<(1, 2, 'm');

  // asserts
  expect(result).toBe(2);
});

// 200% coverage